{
    "name": "evolicraft",
    "preductname": "evolicraft Launcher",
    "version": "1.0.8",
    "description": "Conflictura modded server launcher",
    "main": "src/app.js",
    "author": "Riptiaz/Boulldogo/Vexato/Luuxis/sellkiss",
    "env": "azuriom",
    "settings": "https://evolicraft.online",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=dev electron .",
        "build": "node build.js --obf=true --build=platform",
        "dev": "cross-env-shell DEV_TOOL=open nodemon --ignore AppData/ --ignore node_modules/ --legacy-watch --exec npm start",
        "icon": "node build.js --icon=https://evolicraft.online/storage/img/lg.png"

    },
    "license": "CC0",
    "nodemonConfig": {
        "ext": "js,html,css"
    },
    "dependencies": {
        "discord-rpc": "^4.0.1",
        "electron-updater": "^6.6.2",
        "https": "^1.0.0",
        "minecraft-java-core-azbetter": "^4.1.3",
        "node-fetch": "^2.6.7",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "electron": "^37.2.0",
        "electron-builder": "^26.0.12",
        "nodemon": "^3.1.9",
        "png2icons": "^2.0.1",
        "javascript-obfuscator": "^4.1.0",
        "jimp": "^0.22.12"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Sellkiss/evolifinal2.git"
    }
}
